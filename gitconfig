[credential]
	helper = cache
[core]
	autocrlf = input
	safecrlf = true
	excludesfile = ~/.gitignore_global
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | [%an] %s%d\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[color]
	ui = true
